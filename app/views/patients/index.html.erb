<h1>All Patients</h1>
<ul>
  <% @patients.each do |patient| %>
    <li>
    Patient Name: <%= link_to patient.name, patient_path(patient) %>,
    Age: <%= patient.age %>
    <%= link_to "Delete patient", patient_path(patient), method: :delete, data: { confirm: "Remove #{patient.name}?"} %>

    </li>
  <% end %>
</ul>

<%= link_to "Add new patient", new_patient_path %>
